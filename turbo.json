{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**"]
		},
		"build:prod": {
			"dependsOn": ["^build:prod"],
			"outputs": [".next/**", "dist/**"]
		},
		"start": {
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"db:studio": {
			"cache": false
		},
		"db:generate": {
			"cache": false
		}
	}
}
